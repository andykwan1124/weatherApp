{
    "name": "WeatherMe",
    "version": "1.0",
    "description": "An extension for weather data!",
    "manifest_version": 2,
    "permissions": ["storage", "geolocation"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "My Weather"
    },
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
    
}
